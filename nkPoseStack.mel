/*-
@param $value <float>
@param $min <float>
@param $max <float>
@returns <int>
*/
proc int betweenFloat(float $value, float $min, float $max) {
  return ($min <= $value && $value <= $max);
}

/*-
@param $value <int>
@param $min <int>
@param $max <int>
@returns <int>
*/
proc int betweenInt(int $value, int $min, int $max) {
  return ($min <= $value && $value <= $max);
}

// -----------------------------------------------------------------------------
// 文字列
