proc addPoseButtonPressed() {
  // print("addPoseButtonPressed\n"); // debug
  global int $nkPoseStackInitPasteSetting[];
  global string $nkPoseStackInitSelectSetting[];
  global int $nkPoseStackInitMirrorSetting[];
  // print("  nkPoseStackInitPasteSetting :{" + intArrayToString($nkPoseStackInitPasteSetting, ", ") + "}\n"); // debug
  // print("  nkPoseStackInitSelectSetting:{" + stringArrayToString($nkPoseStackInitSelectSetting, ", ") + "}\n"); // debug
  // print("  nkPoseStackInitMirrorSetting:{" + intArrayToString($nkPoseStackInitMirrorSetting, ", ") + "}\n"); // debug

  global string $nkPoseStackPromptPoseName;
  global string $nkPoseStackPromptNodes[];
  global string $nkPoseStackPromptPoseSettings[];
  initializePromptVariables();

  string $nodes[] = getSelectedNodes();
  // print("  nodes:{\n    " + (stringArrayToString($nodes, ",\n    ")) + "\n  }\n"); // debug
  int $numNodes = `size $nodes`;
  if (!$numNodes) return;

  $nkPoseStackPromptPoseName = makePoseName($nodes[0]);
  $nkPoseStackPromptNodes = $nodes;
  for ($i = 0; $i < $numNodes; $i++) {
    $nkPoseStackPromptPoseSettings[$i] = buildPoseSetting(
      $nkPoseStackInitPasteSetting,
      $nkPoseStackInitSelectSetting,
      $nkPoseStackInitMirrorSetting
    );
  }
  // print("  nkPoseStackPromptPoseName    :" + $nkPoseStackPromptPoseName + "\n"); // debug
  // print("  nkPoseStackPromptNodes       :{\n    " + stringArrayToString($nkPoseStackPromptNodes, ",\n    ") + "\n  }\n"); // debug
  // print("  nkPoseStackPromptPoseSettings:{\n    " + stringArrayToString($nkPoseStackPromptPoseSettings, ",\n    ") + "\n  }\n"); // debug

  string $result = `layoutDialog
      -p (getParentWindow())
      -t "Add Pose"
      -ui "buildPromptWindow"`;

  nkUIRemoveControls("layoutDialog", "");

  // print("  result:" + $result + "\n"); // debug
  // print("    nkPoseStackInitPasteSetting  :{" + intArrayToString($nkPoseStackInitPasteSetting, ", ") + "}\n"); // debug
  // print("    nkPoseStackInitSelectSetting :{" + stringArrayToString($nkPoseStackInitSelectSetting, ", ") + "}\n"); // debug
  // print("    nkPoseStackInitMirrorSetting :{" + intArrayToString($nkPoseStackInitMirrorSetting, ", ") + "}\n"); // debug
  // print("    nkPoseStackPromptPoseName    :" + $nkPoseStackPromptPoseName + "\n"); // debug
  // print("    nkPoseStackPromptNodes       :{\n      " + stringArrayToString($nkPoseStackPromptNodes, ",\n      ") + "\n    }\n"); // debug
  // print("    nkPoseStackPromptPoseSettings:{\n      " + stringArrayToString($nkPoseStackPromptPoseSettings, ",\n      ") + "\n    }\n"); // debug

  if ($result != "OK") return;

  int $numPoses = addPose(
    $nkPoseStackPromptPoseName,
    $nkPoseStackPromptNodes,
    $nkPoseStackPromptPoseSettings
  );

  updatePoseList();
  string $poseList = nkUIGetControl("nkPoseStackWindow", "poseList");
  textScrollListSetSelectIndexedItem($numPoses - 1, $poseList);
}

