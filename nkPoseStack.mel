/*-
@returns <>
*/
proc buildMainWorkspaceControl() {
  // print("buildMainWorkspaceControl\n"); // debug
  int $exists = workspaceControlGetExists("nkPoseStackWindow");
  // print("  exists:" + $exists + "\n"); // debug
  if (!$exists) {
    string $nkPoseStackWindow = `workspaceControl
        -dup false
        -fl true
        -l "nkPoseStack"
        -mw 260
        -rt false
        -ui "catchQuiet (nkPoseStackBuildMainWindow());"
        "nkPoseStackWindow"`;
  }
}

